{
  "name": "lasalle-workshop",
  "version": "0.0.1",
  "description": "Lasalle workshop",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "vc",
    "rule",
    "engine"
  ],
  "author": "F.Hoffmann-La Roche Ltd",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "index.html",
  "scripts": {
    "typecheck": "tsc --pretty --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "lint": "eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint -c .eslintrc.js --ext .js,.jsx,.ts,.tsx ./src --fix",
    "clean": "rimraf build-prod build-dev",
    "start": "npm-run-all --parallel typecheck:watch start:dev mock-server",
    "start:dev": "webpack-dev-server --config ./config/webpack/dev.js",
    "build": "npm run clean && webpack --config ./config/webpack/prod.js && npm run build:copyfiles",
    "build:dev": "npm run clean && webpack --config ./config/webpack/dev.js",
    "build:copyfiles": "copyfiles package.json readme.md LICENSE.txt build-prod",
    "pretest": "rimraf .nyc_output cypress-coverage snapshots jest-coverage reports",
    "test": "npm run test:jest",
    "test:jest": "cross-env NODE_ENV=test jest --verbose --config ./config/jest/jest.config.js --passWithNoTests --no-cache --runInBand --coverage",
    "test:jest:watch": "cross-env NODE_ENV=test jest --verbose --watchAll --config ./config/jest/jest.config.js --no-cache --runInBand",
    "test:cy": "cross-env NODE_ENV=test npm-run-all --parallel --race start cy:run",
    "test:cy:open": "cross-env NODE_ENV=test npm-run-all --parallel start cy:open",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "sonar": "sonar-scanner",
    "auth-server": "node ./mock/server.js",
    "mock-server": "json-server-auth ./mock/db.json --routes ./mock/routes.json --watch ./mock/db.json --id id",
    "deploy:local": "npm run build && cd build-prod && copyfiles -u -V -a ** C:\\InterSystems\\HS\\CSP\\webui\\rule-engine\\"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "test/cypress/steps",
    "cucumberJson": {
      "generate": false,
      "outputFolder": "reports",
      "filePrefix": "cypress.",
      "fileSuffix": ".cucumber"
    }
  },
  "nyc": {
    "report-dir": "cypress-coverage"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@material-ui/core": "^4.10.1",
    "@material-ui/data-grid": "^4.0.0-alpha.12",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "final-form": "^4.19.0",
    "history": "^4.9.0",
    "i18next": "^17.0.13",
    "i18next-xhr-backend": "^3.1.2",
    "immutable": "^3.8.2",
    "md5": "^2.3.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-final-form": "^6.3.3",
    "react-i18next": "^10.12.2",
    "react-redux": "^6.0.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.2.0",
    "react-toast-notifications": "^2.4.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.3.3",
    "@cypress/code-coverage": "^3.7.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.1.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^4.3.5",
    "@types/redux": "^3.6.0",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.16",
    "babel-plugin-istanbul": "^6.0.0",
    "body-parser": "^1.19.0",
    "check-code-coverage": "^1.9.2",
    "copy-webpack-plugin": "^5.0.4",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "cypress-cucumber-preprocessor": "^2.3.1",
    "deep-freeze": "0.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^10.0.0",
    "json-server": "^0.15.1",
    "jsonwebtoken": "^8.5.1",
    "jss": "^9.8.7",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "rimraf": "^3.0.0",
    "sonar-scanner": "^3.1.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.1"
  }
}
